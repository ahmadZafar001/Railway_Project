<div class="container nested-fields">
    <div class="field">
        <%= f.label :phone_number %><br>
        <%= f.number_field :phone_number %>
    </div>
    <div class="field">
        <%= f.label :address %><br>
        <%= f.text_area :address %>
    </div>
    <%= link_to_remove_association "Remove this Address".html_safe, f,class:"btn btn-link"%>
</div>