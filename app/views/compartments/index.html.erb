<p id="notice"><%= notice %></p>

<h1>Listing Compartments</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Capacity</th>
      <th colspan="3"><%= link_to 'New Compartment', new_compartment_path %></th>
    </tr>
  </thead>

  <tbody>
    <% @compartments.each do |compartment| %>
      <tr>
        <td><%= compartment.name %></td>
        <td><%= compartment.capacity %></td>
        <td><%= link_to 'Show', compartment %></td>
        <td><%= link_to 'Edit', edit_compartment_path(compartment) %></td>
        <td><%= link_to 'Destroy', compartment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


